
def isDDM(m, n) :

for i in range(0, n) :

sum = 0
for j in range(0, n) :
sum = sum + abs(m[i][j])

sum = sum - abs(m[i][i])

if (abs(m[i][i]) < sum) :
return False
return True

n = 3
m = [[1,2,-1],
[3,-1,2],

